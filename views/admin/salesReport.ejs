                              </div>
                           </div>

                        </div>

                        <form action="">
                           <div class="row mb-2">
                              <div class="col-2">
                                 <label for="">From:</label>
                                 <input class="form-control m-0 " type="date" name="from" id="">
                              </div>
                              <div class="col-2">
                                 <label for="">To:</label>
                                 <input class="form-control m-0 " type="date" name="to" id="">
                              </div>
                              <div class="col-1">
                                 <label for=""></label>
                                 <input class="form-control m-0 btn bg-dark" type="submit" value="Filter">
                              </div>
                           </div>
                        </form>

                        <div class="table-responsive">
                           <table class="table">
                              <thead>

                                 <tr>

                                    <th scope="col">SL No. </th>
                                    <th scope="col">Date </th>
                                    <th scope="col">Total Amount </th>
                                    <th scope="col">No. Products </th>
                                    <th scope="col">Payment Method</th>
                                    <th scope="col">Status </th>

                                 </tr>
                              </thead>


                              <tbody>

                                 <% orders.forEach( (order, index)=> { %>
                                    <tr>

                                       <th scope="col">
                                          <%= index+1 %>
                                       </th>
                                       <td scope="col">
                                          <%= order.orderDate.toISOString().substring(0,10) || 'N/A' %>
                                       </td>
                                       <td scope="col">
                                          <%= order.totalPrice %>
                                       </td>
                                       <td scope="col">
                                          <%= order.products.length %>
                                       </td>
                                       <td scope="col">
                                          <% if (order.paymentMethod=='razorpay' ) {%>
                                             Online
                                             <% } else { %>
                                                Cash On Ddelivery
                                                <% } %>
                                       </td>


                                       <td scope="col"><span class="dot"
                                             style="background:  #13f613 !important; color: #13f613; "></span>
                                          Deliverd </td>
                                       <td scope="col">

                                       </td>
                                    </tr>

                                    <% }) %>

                              </tbody>

                           </table>

                           <p>
                           </p>

