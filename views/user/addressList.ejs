         <div class="col-md-9">
            
               <div class="col-md-6">
                  <div class="osahan-account-page-left shadow-sm bg-white h-100">
                     <div class="border-bottom p-4">
                        <div class="osahan-user text-center">
                           <div class="osahan-user-media">
                              <img class="mb-3 rounded-pill shadow-sm mt-1"
                                 src="https://bootdey.com/img/Content/avatar/avatar7.png" alt="gurdeep singh osahan">
                              <div class="osahan-user-media-body">
                                 <h6 class="mb-2">
                                    <%= user.firstName %>
                                       <%= user.lastName %>
                                 </h6>
                                 <p class="mb-1">
                                    <%= user.phone %>
                                 </p>
                                 <p><a href="/cdn-cgi/l/email-protection" class="__cf_email__"
                                       data-cfemail="8ce5ede1e3ffede4ede2ccebe1ede5e0a2efe3e1">
                                       <%= user.email %>
                                    </a>
                                 </p>
                                 <p class="mb-0 text-black font-weight-bold"><a class="text-primary mr-3"
                                       href="/addAddress">
                                       <span class="material-symbols-outlined">post_add</span> Address</a></p>
                              </div>
                           </div>
                        </div>
                     </div>
                     <ul class="nav nav-tabs flex-column border-0 pt-4 pl-4 pb-4" id="myTab" role="tablist">
                        <li class="nav-item">
                           <a class="nav-link" id="addresses-tab" data-toggle="tab" href="#addresses" role="tab"
                              aria-controls="addresses" aria-selected="false"><i class="icofont-location-pin"></i>
                              Addresses</a>
                        </li>
                     </ul>
                  </div>
               </div>

            </div>
            

               <div class="col-md-12">
                  <div class="osahan-account-page-right shadow-sm bg-white p-4 h-100">
                     <div class="tab-content" id="myTabContent">
                        <div class="tab-pane fade  active show" id="addresses" role="tabpanel"
                           aria-labelledby="addresses-tab">
                           <h4 class="font-weight-bold mt-0 mb-4">Manage Addresses</h4>

                           <div class="row">
                              <% if ( typeof data=='undefined' ) { %>
                                 <% address.forEach ( function (address) { %>
                                    <div class="col-md-6">
                                       <div class="bg-white card addresses-item mb-4 border border-primary shadow">
                                          <div class="gold-members p-4">
                                             <div class="media">
                                                <div class="mr-3"><i class="icofont-ui-home icofont-3x"></i></div>
                                                <div class="media-body">
                                                   <h6 class="mb-1 text-secondary">
                                                      <%= address.type %>
                                                   </h6>
                                                   <h6 class="mb-1 text-dark"><strong>
                                                         <%= address.firstName %>
                                                            <%= address.lastName %>
                                                      </strong></h6>
                                                   <p class="text-black">
                                                      <%= address.address %>, <%= address.street %> , <%= address.city
                                                               %> ,
                                                               <%= address.state %>, <%= address.country %>,
                                                                  <%= address.zipCode %>, <%= address.phone %>
                                                   </p>
                                                   <p class="mb-0 text-black font-weight-bold"><a
                                                         class="text-primary mr-3"
                                                         href="/editAddress?id=<%=address._id%>"><span
                                                            class="material-symbols-outlined">
                                                            edit_note
                                                         </span></a>
                                                      <a class="text-danger"
                                                         href="/deleteAddress?id=<%=address._id%>"><span
                                                            class="material-symbols-outlined">
                                                            delete
                                                         </span></a>
                                                   </p>
                                                </div>
                                             </div>
                                          </div>
                                       </div>
                                    </div>

                                    <% }) %>

                                       <% } else { %>

                                          <span>Your Have Not Added Address.</span>

                                          <a href="/addAddress">
                                             <span class="addtocart">
                                                <button class="btn btn-primary btn-addtocart d-inline">
                                                   Try Adding
                                                </button>
                                             </span>
                                          </a>

                                          <% } %>
                           </div>

                        </div>
                     </div>
                  </div>
               
         </div>
      </div>
