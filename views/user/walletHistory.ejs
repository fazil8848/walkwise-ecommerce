                <div class="container col-lg-9 py-5">
                    <div class="row d-flex justify-content-center align-items-center">
                        <div class="col">
                            <table class="table table-hover "
                                style="border-radius: 5px; background-color: #88c8bc; color: #fff;">
                                <thead>
                                    <tr>
                                        <th scope="col">SL</th>
                                        <th scope="col">Amount</th>
                                        <th scope="col">Date</th>
                                        <th scope="col">Transaction</th>
                                        <th scope="col">Description</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% if (typeof history !== 'undefined' ) {
                                        history.forEach( (history,i) =>{ %>
                                    <tr>
                                        <th scope="row"><%= i+1 %></th>
                                        <td><%= history.amount %></td>
                                        <td><%= history.date.toISOString().substring(0,10) %></td>
                                        <td><%= history.transaction %></td>
                                        <td><%= history.description %></td>
                                    </tr>
                                    <% })} else { %>
                                        <tr>
                                            <td>
                                                No Wallet Transaction Done.
                                            </td>
                                        </tr>
                                    <% } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
