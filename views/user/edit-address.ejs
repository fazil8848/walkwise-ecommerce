		<div class="container py-5">
			<div class="card-body p-md-5 text-black">
				<form method="post" class="colorlib-form">
					<h2 style="margin-bottom: 10px;">Billing Details</h2>
					<p style=" font-size: smaller; font-weight: 100;">All the <span class="star">*</span> <span>
							Marked fields are required</span>
					</p>
					<div class="row">
						<div class="col-md-12">
							<div class="form-group">
								<input type="hidden" name="id" value="<%= data._id %>">

								<div class="row">

									<div class="form-field">
										<% if (typeof message !=='undefined' ) { %>
											<div class="form-group">
												<p style="color: red;">
													<%= message %>
												</p>
											</div>
											<% } %>
									</div>

									<div class="col-md-6">

										<label for="country">Select Country <span class="star">*</span></label>
										<div class="form-field">
											<i class="icon icon-arrow-down3"></i>
											<select required name="country" id="people" class="form-control">
												<option value="<%= data.country %>"><%= data.country %></option>
												<option value="India">India</option>
												<option value="Alaska">Alaska</option>
												<option value="China">China</option>
												<option value="Japan">Japan</option>
												<option value="Korea">Korea</option>
												<option value="Philippines">Philippines</option>
											</select>
										</div>

									</div>

									<div class="col-md-6">
										<div class="form-group">
											<label for="country">Select Type Of address <span
													class="star">*</span></label>
											<div class="form-field">
												<i class="icon icon-arrow-down3"></i>
												<select class="form-control" name="type">
													<option value="<%= data.type %>"> <%= data.type %> </option>
													<option value="Home"> Home </option>
													<option value="work"> Work </option>

												</select>
											</div>
										</div>
									</div>
								</div>

								<div class="row">
									<div class="col-md-6 mb-4">
										<div class="form-field">
											<label class="form-label" for="form3Example1m">First name <span class="star">*</span> </label>
											<input type="text" oninput="capitalizeFirstLetter(this)"
												value="<%= data.firstName %>" name="firstName" id="form3Example1m"
												class="form-control form-control-lg" />
										</div>
									</div>
									<div class="col-md-6 mb-4">
										<div class="form-field">
											<label class="form-label" for="form3Example1n">Last name <span class="star">*</span> </label>
											<input type="text" oninput="capitalizeFirstLetter(this)"
												value="<%= data.lastName %>" name="lastName" id="form3Example1n"
												class="form-control form-control-lg" />
										</div>
									</div>
								</div>

								<div class="form-field mb-4">
									<label class="form-label" for="form3Example8">Address <span class="star">*</span> </label>
									<input type="text" oninput="capitalizeFirstLetter(this)" value="<%= data.address %>"
										name="address" id="form3Example8" class="form-control form-control-lg" />
								</div>

								<div class="row">
									<div class="col-md-6 mb-4">
										<div class="form-field">
											<label class="form-label" for="form3Example1m">State <span class="star">*</span> </label>
											<input type="text" oninput="capitalizeFirstLetter(this)"
												value="<%= data.state %>" name="state" id="form3Example1m"
												class="form-control form-control-lg" />
										</div>
									</div>
									<div class="col-md-6 mb-4">
										<div class="form-field">
											<label class="form-label" for="form3Example1n">City <span class="star">*</span> </label>
											<input type="text" oninput="capitalizeFirstLetter(this)"
												value="<%= data.city %>" name="city" id="form3Example1n"
												class="form-control form-control-lg" />
										</div>
									</div>
								</div>
								
								<div class="form-field mb-4">
									<label class="form-label" for="form3Example3">Street <span class="star">*</span> </label>
									<input type="text" oninput="capitalizeFirstLetter(this)" value="<%= data.street %>"
										name="street" id="form3Example3" class="form-control form-control-lg" />
								</div>

								<div class="form-field mb-4">
									<label class="form-label" for="form3Example3">Zip <span class="star">*</span> </label>
									<input type="text" oninput="capitalizeFirstLetter(this)" value="<%= data.zipCode %>"
										name="zipCode" id="form3Example3" class="form-control form-control-lg" />
								</div>

								<div class="form-field mb-4">
									<label class="form-label" for="form3Example2">Phone <span class="star">*</span> </label>
									<input type="text" oninput="capitalizeFirstLetter(this)" value="<%= data.phone %>"
										name="phone" id="form3Example2" class="form-control form-control-lg" />
								</div>

								<div class="d-flex justify-content-end pt-3">
									<button type="submit" class="btn btn-success btn-lg ms-2"
										style="background-color:hsl(210, 100%, 50%)">Update Address</button>
								</div>
				</form>
			</div>
		</div>



