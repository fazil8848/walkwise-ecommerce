	<script>
		$(document).ready(function () {
			$('#paginationForm').submit(function (event) {
				event.preventDefault();
	
				const formData = $(this).serializeArray();
				const currentPage = parseInt($('input[name="page"]', this).val());
				const dynamicURL = generatePageLink(currentPage, formData);
	
				window.location.href = dynamicURL;
			});
		});
	
		function generatePageLink(page, queryParams) {
			const queryString = queryParams
				.filter(param => param.name !== 'page')
				.map(param => param.name + '=' + encodeURIComponent(param.value))
				.join('&');
	
			return '/shop?page=' + page + (queryString ? '&' + queryString : '');
		}
	</script>

